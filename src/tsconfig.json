{
    "compilerOptions": {
        "target": "es5",
        
        // values for Node 
        //"module": "commonjs",
        //"outDir": "../build",
        
        // values for Browser
        "module": "amd", // none or commonjs (Node style) or amd (require.js) or es6 (ECMAScript 2015 native modules)
        "outFile": "../build/build.js", // amd and system modules only
        
        "sourceMap": true,
        "allowJs": false,
        "strictNullChecks": false,
        //"noImplicitAny" : true, 
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "excel_com.ts"
    ]
}
